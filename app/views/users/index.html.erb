<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= will_paginate %>



<table class="table table-condensed">
  <thead>
    <tr>
      <th>Name user</th>
      <th>Email</th>
      <th>Orders</th>
    </tr>
  </thead>
  <tbody>

    <% @users.each do |user| %>
      <tr>
        <td><%= link_to "#{user.name}", user %></td>
        <td><%= link_to "#{user.email}", user %></td>
        <td><%= link_to "#{}", user %></td>
      </tr> 
    <% end %>
  </tbody>
</table>

<%= will_paginate %>